{
  "expo": {
    "name": "directTickets",
    "slug": "directTickets",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/directtickets.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/directtickets.png",
        "backgroundColor": "#000000"
      },
      "package": "com.pavankumar_07s.directTickets"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/directtickets.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/directtickets.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f62e8b9b-2156-4821-8da6-037ee7f1a817"
      }
    }
  }
}
